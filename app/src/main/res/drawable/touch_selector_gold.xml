<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- State when a row is being pressed, but hasn't yet been activated (finger down) -->
    <item android:state_pressed="true" android:drawable="@drawable/hexagon_gold_pressed" />

    <item android:state_activated="true" android:drawable="@drawable/hexagongold" />

    <!-- Default, "just hangin' out" state. -->
    <item android:drawable="@drawable/hexagongold" />
</selector>